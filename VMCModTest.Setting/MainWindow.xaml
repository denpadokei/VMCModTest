<Window x:Class="VMCModTest.Setting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VMCModTest.Setting"
        xmlns:vm="clr-namespace:VMCModTest.Setting.ViewModels"
        xmlns:view="clr-namespace:VMCModTest.Setting.Views"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="450" Width="800">
    <Window.DataContext>
        <vm:MainWindowViewModel />
    </Window.DataContext>
    <Grid>
        <ContentControl>
            <view:MainControll />
        </ContentControl>
    </Grid>
</Window>
